{
  "name": "cms-sanity",
  "version": "1.11.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@sanity/block-content-to-react": "2.0.7",
    "@sanity/client": "1.149.18",
    "@sanity/image-url": "0.140.19",
    "babel-plugin-macros": "^3.0.1",
    "classnames": "2.2.6",
    "date-fns": "2.15.0",
    "framer-motion": "^3.1.1",
    "next": "latest",
    "next-sanity": "^0.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.7.0",
    "react-icons": "^4.1.0",
    "swr": "^0.2.3",
    "twin.macro": "^2.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@sanity/cli": "^2.0.5",
    "autoprefix": "^1.0.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "tailwindcss": "^2.0.2"
  }
}
